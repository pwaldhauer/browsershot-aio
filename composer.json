{
    "name": "pwaldhauer/browsershot-aio",
    "description": "All in one Docker Container to run Browsershot",
    "keywords": [
        "laravel",
        "browsershot",
        "docker"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Philipp Waldhauer",
            "email": "hi@pwa.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/browsershot": "^3.52"
    },
    "autoload": {
        "psr-4": {
            "pwaio\\BrowsershotAio\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}